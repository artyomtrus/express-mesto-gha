{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-console": "off",
      "consistent-return": "off",
      "no-underscore-dangle": ["off", { "allow": ["_id", "_bar"] }]
    }
}
